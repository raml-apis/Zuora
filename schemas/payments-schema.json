{
  "$schema": "http://json-schema.org/draft-03/schema",
  "required": false,
  "type": "object",
  "properties": {
    "success": {
      "required": false,
      "type": "boolean"
    },
    "payments": {
      "required": false,
      "type": "array",
      "items": {
        "required": false,
        "type": "object",
        "properties": {
          "accountId": {
            "required": false,
            "type": "string"
          },
          "accountName": {
            "required": false,
            "type": "string"
          },
          "accountNumber": {
            "required": false,
            "type": "string"
          },
          "amount": {
            "required": false,
            "type": "number"
          },
          "effectiveDate": {
            "required": false,
            "type": "string"
          },
          "gatewayTransactionNumber": {
            "required": false,
            "type": "null"
          },
          "id": {
            "required": false,
            "type": "string"
          },
          "paidInvoices": {
            "required": false,
            "type": "array",
            "items": {
              "required": false,
              "type": "object",
              "properties": {
                "appliedPaymentAmount": {
                  "required": false,
                  "type": "integer"
                },
                "invoiceId": {
                  "required": false,
                  "type": "string"
                },
                "invoiceNumber": {
                  "required": false,
                  "type": "string"
                }
              }
            }
          },
          "paymentMethodId": {
            "required": false,
            "type": "string"
          },
          "paymentNumber": {
            "required": false,
            "type": "string"
          },
          "status": {
            "required": false,
            "type": "string"
          },
          "type": {
            "required": false,
            "type": "string"
          }
        }
      }
    }
  }
}